# vim: syntax=dosini
include "/usr/share/kbd/keymaps/i386/qwerty/br-abnt2.map.gz"

keycode 1 = Caps_Lock
keycode 58 = Escape

# 1 2 9 0
altgr keycode 2 = bracketleft
altgr keycode 3 = bracketright
altgr keycode 10 = braceleft
altgr keycode 11 = braceright

# q w e r t y u i o p
altgr keycode 16 = period
altgr keycode 17 = backslash
altgr keycode 18 = bar
altgr keycode 19 = colon
altgr keycode 20 = parenleft
altgr keycode 21 = parenright
altgr keycode 22 = underscore
altgr keycode 23 = minus
altgr keycode 24 = equal
altgr keycode 25 = plus

# a s d f g h i j k l
altgr keycode 30 = asciitilde
altgr keycode 31 = slash
altgr keycode 32 = quotedbl
altgr keycode 33 = asterisk
altgr keycode 34 = comma
altgr keycode 35 = ampersand
altgr keycode 36 = dollar
altgr keycode 37 = percent
altgr keycode 38 = asciicircum

# z x c v b n m
altgr keycode 44 = question
altgr keycode 45 = numbersign
altgr keycode 46 = exclam
altgr keycode 47 = semicolon
altgr keycode 48 = comma
altgr keycode 49 = bracketleft
altgr keycode 50 = braceright
